{
  "name": "tinydice",
  "version": "0.0.3",
  "description": "Tiny library for dice rolls",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "./lib/tinydice.umd.cjs",
  "module": "./lib/tinydice.js",
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/tinydice.js",
      "require": "./lib/tinydice.umd.cjs"
    }
  },
  "scripts": {
    "clean": "del lib",
    "test": "vitest",
    "dev": "vite build --watch",
    "build": "vite build"
  },
  "keywords": [
    "dice",
    "random"
  ],
  "author": "Matt Styles",
  "license": "MIT",
  "devDependencies": {
    "del-cli": "^5.0.0",
    "tsconfig": "workspace:*",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "2.0.0-beta.3",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "just-random-integer": "^4.2.0"
  }
}
